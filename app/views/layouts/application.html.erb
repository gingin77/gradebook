<!DOCTYPE html>
<html>
  <head>
    <title>Gradebook</title>
    <%= csrf_meta_tags %>

    <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload' %>
  </head>

  <body>
    <nav>
      <% if current_user %>
        Welcome, <%= current_user.get_proper_name %> | <%= link_to "Logout", logout_path, :method => :delete %>
      <% else %>
      <% end %>
    </nav>
    <%= yield %>
  </body>
</html>
