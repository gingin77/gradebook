<h1>Gradebook Login Page</h1>
<%= form_tag login_path do %>
  <%= label_tag :username %>
  <%= text_field_tag :username, params[:username], class: 'form-control' %>

  <%= label_tag :password %>
  <%= password_field_tag :password, class: 'form-control' %>

  <div class="submit-button"><%= submit_tag "LOGIN" %></div>
<% end %>
